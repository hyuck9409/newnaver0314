package programmers;

class Solution {
    public int solution(int angle) {
        int answer = 0;
        if(angle<90)
        {
        	System.out.println(1);
        }else if(angle==90)
        {
        	System.out.println(2);
        }else if(angle<180)
        {
        	System.out.println(3);
        }else if(angle==180) {
        	System.out.println(4);
        }
        
        return answer;
    }
}